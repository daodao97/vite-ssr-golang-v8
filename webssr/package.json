{
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite --port 3333 --open",
    "lint": "eslint .",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest",
    "up": "taze major -I",
    "build": "pnpm run build:client && pnpm run build:server",
    "build:client": "vite build --ssrManifest --outDir ./dist/client --emptyOutDir",
    "build:server": "vite build -c vite.config.prod.ts --outDir ./dist/server --emptyOutDir"
  },
  "dependencies": {
    "@vue/server-renderer": "^3.5.22",
    "@vueuse/core": "^14.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-vue-next": "^0.552.0",
    "pinia": "^3.0.4",
    "radix-vue": "^1.9.17",
    "reka-ui": "^2.6.0",
    "tailwind-merge": "^3.3.1",
    "tw-animate-css": "^1.4.0",
    "vue": "^3.5.22",
    "vue-i18n": "^11.1.12",
    "vue-router": "^4.6.3",
    "vue3-google-signin": "^2.1.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.2.0",
    "@iconify-json/carbon": "^1.2.14",
    "@tailwindcss/vite": "^4.1.16",
    "@types/node": "^24.10.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue-macros/volar": "^3.1.1",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.39.1",
    "eslint-plugin-format": "^1.0.2",
    "jsdom": "^27.1.0",
    "tailwindcss": "^4.1.16",
    "taze": "^19.9.0",
    "typescript": "^5.9.3",
    "unplugin-auto-import": "^20.2.0",
    "unplugin-vue-components": "^30.0.0",
    "unplugin-vue-macros": "^2.14.5",
    "unplugin-vue-router": "^0.16.1",
    "vite": "^7.2.0",
    "vite-plugin-vue-layouts": "^0.11.0",
    "vitest": "^4.0.7",
    "vue-tsc": "^3.1.3"
  },
  "packageManager": "pnpm@10.20.0"
}
